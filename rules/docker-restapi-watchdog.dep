DPATH       := $($(DOCKER_RESTAPI_WATCHDOG)_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/docker-restapi-watchdog.mk rules/docker-restapi-watchdog.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
DEP_FILES   += $(shell git ls-files $(DPATH))

$(DOCKER_RESTAPI_WATCHDOG)_CACHE_MODE  := GIT_CONTENT_SHA
$(DOCKER_RESTAPI_WATCHDOG)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(DOCKER_RESTAPI_WATCHDOG)_DEP_FILES   := $(DEP_FILES)

$(eval $(call add_dbg_docker,$(DOCKER_RESTAPI_WATCHDOG),$(DOCKER_RESTAPI_WATCHDOG_DBG)))
