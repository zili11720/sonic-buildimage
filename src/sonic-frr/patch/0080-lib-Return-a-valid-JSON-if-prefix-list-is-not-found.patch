diff --git a/lib/plist.c b/lib/plist.c
index 0444cfe2d3..cfd1ae0575 100644
--- a/lib/plist.c
+++ b/lib/plist.c
@@ -1088,7 +1088,7 @@ static int vty_show_prefix_list(struct vty *vty, afi_t afi, const char *name,
 		plist = prefix_list_lookup(afi, name);
 		if (!plist) {
 			if (uj)
